    <style name="AppTheme" parent="AppTheme.Base"/>
    <style name="AppTheme.Base" parent="Theme.Material3.Dark.NoActionBar">
        <item name="colorPrimary">@color/color_primary</item>
        <item name="colorPrimaryDark">@color/color_primary_dark</item>
        <item name="colorAccent">@color/color_accent</item>
        <item name="windowActionModeOverlay">true</item>
    </style>
    <style name="KwKeyboard">
        <item name="itemTextAppearance">?attr/itemTextAppearance</item>
    </style>
    <style name="KwKeyboardView" parent="KwKeyboard">
        <item name="keyTextSize">@dimen/keyboard_text_size</item>
    </style>
    <style name="MyKeyboardViewPopup" parent="KwKeyboard">
        <item name="keyTextSize">@dimen/preview_text_size</item>
    </style>
    <style name="Theme.Research" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        
        <item name="colorPrimary">@color/color_primary</item>
        <item name="colorPrimaryVariant">@color/color_primary</item>
        <item name="colorOnPrimary">@color/white</item>
        
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        
        <item name="android:statusBarColor" ns1:targetApi="l">?attr/colorPrimaryVariant</item>
        

    </style>
    <declare-styleable name="KeyboardViewPreviewState">
        <attr format="boolean" name="state_long_pressable"/>
    </declare-styleable>
    <declare-styleable name="KwKeyboard">
        <attr format="fraction" name="keyWidth"/>
        <attr format="fraction" name="horizontalGap"/>
    </declare-styleable>
    <declare-styleable name="KwKeyboardView">
        <attr format="dimension" name="keyTextSize"/>
    </declare-styleable>
    <declare-styleable name="KwKeyboard_Key">
        
        <attr format="integer" name="code"/>
        
        <attr format="reference" name="popupKeyboard"/>
        
        <attr format="string" name="popupCharacters"/>
        
        <attr name="keyEdgeFlags">
            <!-- Key is anchored to the left of the keyboard. -->
            <flag name="left" value="1"/>
            <!-- Key is anchored to the right of the keyboard. -->
            <flag name="right" value="2"/>
        </attr>
        
        <attr format="boolean" name="isRepeatable"/>
        
        <attr format="string" name="keyLabel"/>
        
        <attr format="reference" name="keyIcon"/>
        
        <attr format="string" name="topSmallNumber"/>
    </declare-styleable>
