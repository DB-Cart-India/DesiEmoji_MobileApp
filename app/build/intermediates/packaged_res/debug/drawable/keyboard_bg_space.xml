<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <layer-list>
            <item android:id="@+id/space_pressed" android:bottom="@dimen/frogo_dimen_6dp" android:left="@dimen/frogo_dimen_6dp" android:right="@dimen/frogo_dimen_6dp" android:state_pressed="true" android:top="@dimen/frogo_dimen_6dp">
                <shape android:shape="rectangle">
                    <solid android:color="@color/md_grey_600" />
                    <corners android:radius="@dimen/frogo_dimen_8dp" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item>
        <layer-list>
            <item android:id="@+id/space_normal" android:bottom="@dimen/frogo_dimen_6dp" android:left="@dimen/frogo_dimen_6dp" android:right="@dimen/frogo_dimen_6dp" android:state_pressed="true" android:top="@dimen/frogo_dimen_6dp">
                <shape android:shape="rectangle">
                    <stroke android:color="@color/keyboard_bg_normal_stroke_color"
                        android:width="1dp"/>
                    <solid android:color="@color/keyboard_bg_normal_color" />
                    <corners android:radius="@dimen/frogo_dimen_8dp" />
                </shape>
            </item>
        </layer-list>
    </item>
</selector>
